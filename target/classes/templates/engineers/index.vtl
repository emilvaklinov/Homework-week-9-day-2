<p class="pageHeader">Manager</p>

<table>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Salary</th>
        <th>Department</th>

    </tr>

    #foreach ($engineer in $engineers)
    <tr>
        <th>$engineer.getFirstName()</th>
        <th>$engineer.getLastName()</th>
        <th>$engineer.getSalary()</th>
        <th>$engineer.getDepartment().getTitle()</th>

    </tr>
    #end
</table>